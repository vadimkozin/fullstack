{
  "name": "fullstack",
  "version": "1.0.0",
  "description": "sample fullstack app",
  "main": "index.js",
  "engines": {
    "node": "10.5.0",
    "npm": "6.3.0"
  },
  "scripts": {
    "start": "node index",
    "start_prod": "NODE_ENV=production node index",
    "server": "nodemon index",
    "server2": "./kill_proc_port_5000.sh && nodemon index",
    "client-install": "npm install --prefix client",
    "client": "npm run start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "dev2": "concurrently \"npm run server2\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm run client-install && npm run build-prod --prefix client"
  },
  "keywords": [
    "mongo",
    "express",
    "angular",
    "node"
  ],
  "author": "VK",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "cors": "^2.8.5",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.24.0",
    "mongoose": "^5.4.20",
    "morgan": "^1.9.1",
    "multer": "^1.4.1",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0"
  },
  "devDependencies": {
    "concurrently": "^3.6.1",
    "nodemon": "^1.18.10"
  }
}
